<script type="text/javascript">
    $(document).ready(function(){
     
        var pdf = new jsPDF('landscape','cm','a4');
        var pdf2 = new jsPDF('landscape','cm','a4');

             var options = { };
        
            var elementHandler = {
                 '#cmd': function (element, renderer) {
                  return true;
                   },
                 '#cmd2': function (element, renderer) {
                  return true;
                   }
            };

            var specialElementHandlers = {
                '#bypassme': function (element, renderer) {
                    return true;
                }
            };
           

        $('#cmd').click(function () {
    
            pdf.addHTML($('#wrap').first(),options,function() {
                
                pdf.save("titulo_portada.pdf");
                 pdf.output('datauristring');
            });
            
        });

        $('#cmd2').click(function () {
    
            pdf.addHTML($('#back').first(),options,function() {
                
                pdf.save("titulo_reverso.pdf");
                 pdf.output('datauristring');
            });
            
        });


    });
</script>